<Window x:Class="HarryManual.CategotyAdditionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HarryManual"
        mc:Ignorable="d"
        Title="CategotyAdditionWindow" Height="130" Width="320">
    <Grid>
        <Label Content="Добавление категории" HorizontalAlignment="Center" VerticalAlignment="Top"/>
        <GroupBox Header="Название" Margin="0,19,0,0">
            <StackPanel>
                <TextBox x:Name="categoryName" TextWrapping="Wrap" Width="289"/>
                <Button Content="Добавить" Margin="0,10,0,0" Click="Button_Click"/>

            </StackPanel>
        </GroupBox>

    </Grid>
</Window>
